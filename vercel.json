{
  "buildCommand": "npm run build",
  "outputDirectory": "claude-flow/intelligent-searcher/client/build",
  "framework": null,
  "functions": {
    "api/*.py": {
      "runtime": "python3.9"
    }
  },
  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "/api/$1"
    },
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ]
}
